{
  "name": "genshin-feedback",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=local concurrently \"npm run start\" \"cd frontend && npm run start\"",
    "start": "node ./bin/www",
    "build-frontend": "cd frontend && npm run build",
    "build-copy-files": "cp -a frontend/build/static static",
    "build-start": "npm run build-frontend && npm run build-copy-files && npm start"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "passport": "^0.5.2",
    "passport-google-oidc": "^0.1.0",
    "sequelize": "^6.19.0"
  }
}
